function destroyer(arr) {

  var result = arr ;
   var myFilter =  function(elem, j, arr) {
    return(elem!=arg);
                   };

  for(var i = 1; i<arguments.length; i++){
    var arg = arguments[i];
   
    result = result.filter(myFilter); 
  }    
  return result;                           
}    
destroyer([1, 2, 3, 1, 2, 3], 2, 3);

